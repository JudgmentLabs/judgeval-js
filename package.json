{
  "name": "judgeval",
  "version": "0.2.1",
  "description": "Judgment SDK for TypeScript/JavaScript",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.22.0",
    "@langchain/anthropic": "^0.3.17",
    "@langchain/community": "^0.3.40",
    "@langchain/core": "^0.3.44",
    "@langchain/langgraph": "^0.2.63",
    "@langchain/openai": "^0.5.5",
    "@supabase/supabase-js": "^2.43.4",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "ansi-colors": "^4.1.3",
    "axios": "^1.8.4",
    "chalk": "4.1.2",
    "cli-progress": "^3.12.0",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "eventsource-parser": "^1.1.2",
    "js-yaml": "^4.1.0",
    "json-schema-to-ts": "^3.1.0",
    "langchain": "^0.3.21",
    "node-fetch": "^2.7.0",
    "papaparse": "^5.5.2",
    "undici-types": "^6.21.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.1"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.22.0",
    "openai": "^4.0.0",
    "together-ai": "^0.7.0"
  },
  "devDependencies": {
    "@types/ansi-colors": "^3.2.0",
    "@types/cli-progress": "^3.11.6",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/papaparse": "^5.3.15",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build:dev": "rm -rf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build:prod": "cross-env NODE_ENV=production rm -rf dist && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build": "npm run build:prod",
    "build:examples": "tsc -p tsconfig.examples.json",
    "build:e2etests": "tsc -p tsconfig.e2etests.json",
    "test": "jest",
    "test:e2e": "jest e2etests",
    "test:e2e:eval": "jest --config jest.config.js src/e2etests/eval-operations.test.ts",
    "test:e2e:traces": "jest --config jest.config.js src/e2etests/judgee-traces.test.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build:prod",
    "docs": "typedoc --out docs src/index.ts",
    "demo:basic": "npx ts-node src/demo/basic-bot.ts",
    "demo:llm-wrap": "npx ts-node src/demo/llm-wrap-demo.ts",
    "demo:openai-wrap": "npx ts-node src/demo/openai-wrap-demo.ts",
    "demo:complex-async": "npx ts-node src/demo/demo_complex_async.ts",
    "example:basic": "npm run build && node dist/examples/basic-usage.js",
    "example:result-retrieval": "npm run build && node dist/examples/result-retrieval.js",
    "example:custom-scorer": "npm run build && node dist/examples/custom-scorer.js",
    "example:compare-python": "npm run build && node dist/examples/compare-with-python.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JudgmentLabs/judgeval-js.git"
  },
  "keywords": [
    "judgment",
    "evaluation",
    "llm",
    "ai",
    "sdk"
  ],
  "author": "Judgment AI",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JudgmentLabs/judgeval-js/issues"
  },
  "homepage": "https://judgmentlabs.ai",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  }
}
